    "eslint": "8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.0.14",
    "eslint-plugin-check-file": "^2.8.0",
    "eslint-plugin-header": "^3.1.1",
