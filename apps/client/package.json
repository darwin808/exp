    "react-dom": "^18.3.1",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.24.0",
    "react-select": "^5.8.0",
    "react-tooltip": "^5.28.0",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@originjs/vite-plugin-federation": "^1.3.5",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
